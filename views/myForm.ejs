

<div class="wrapper-postingan">
  <% forms.forEach(form => { %>
  <a href="/detailMyForm/<%= form.form_id %>" class="postingan">
    <div class="top">
      <div class="judul">
        <p class="judul-form"><%= form.title %></p>
      </div>
      <div class="deskripsi"><%= form.description %></div>
    </div>
    <div class="bottom">
      <div class="pengguna">
        <div class="pic-uploader">
          <% if (form.avatar) { %>
          <img src="./img/<%= form.avatar %>" width="100" height="100" />
          <% } else { %>
          <img src="./img/boy.png" width="100" height="100" />
          <% } %>
        </div>
        <div class="nama-uploader">
          <span class="nama"><%= form.username %> </span><span>|</span
          ><span> <%= moment(form.created_at).format('LL') %></span>
        </div>
      </div>
    </div>
  </a>
  <% }); %>
</div>
